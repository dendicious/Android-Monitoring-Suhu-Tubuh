<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.graph"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="19" />
		<uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.app.dendi_tshirt.SplashScreen"
            android:label="@string/app_name"
            android:icon="@drawable/ic_launcher" 
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name="com.app.dendi_tshirt.MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name="com.app.dendi_tshirt.TentangPengembang"
            android:label="@string/btn_tentang_pengembang" >
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name="com.app.dendi_tshirt.SuhuRealtime"
            android:label="@string/btn_realtime" >
        </activity>
        <activity
            android:screenOrientation="landscape"
            android:name="com.app.dendi_tshirt.GrafikHarian"
            android:label="@string/btn_mulai_pemantauan" >
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name="com.app.dendi_tshirt.Login"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name="com.app.dendi_tshirt.LoginGagal"
            android:label="@string/app_name" >
        </activity>
        
    </application>

</manifest>
